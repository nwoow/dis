<%= simple_form_for(@food_intake) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :food_intake_name, label: "Food Intake", error: "Please enter a food intake text"%>
    <!-- <%= f.input :status , error: "status is mandatory" %> -->
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Save" %>
  </div>
<% end %>
